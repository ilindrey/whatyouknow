    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver"
            ],
            "django": true
        },
        {
            "name": "whatyouknow: data generation",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/data_gen",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": [
                "--factor=1"
            ],
        },
    ]
}